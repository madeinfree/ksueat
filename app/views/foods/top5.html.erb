<h1 class="text-center">崑山美食資訊卡 - Top5</div>
<% @foods.each { |food| %>
  <div id="food-border" class="row">
      <div class="span12">
        <h1 class="text-center"> <%= link_to("永康美食・#{food.name}", food_path(food)) %> </h1>
      </div>
      <div class="span4">
        <h1 class="text-center"> <img class="img-polaroid" src="data:image/png;base64," /> </h1>
      </div>
      <div class="span6">
        <h3>營業時間：<%= food.traffictimeopen.strftime('%H:%M:%S') %> ~ <%= food.traffictimeclose.strftime('%H:%M:%S') %></h3>
        <h3>電話：<%= food.phone %></h3>
        <h3>地址：<%= food.address %></h3>
        <h3>星數：<%= food.foodpoints.count %> 星</h3>
        <h3>評論條：<%= food.comments.count %> 張</h3>
      </div>
  </div>
<% } %>